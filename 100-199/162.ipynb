    "N = 16\n",
    "\n",
    "res = 0\n",
    "\n",
    "for d in range(1, N + 1):\n",
    "    res += 15 ** (d-1) * 14 * 2 + 15 ** d\n",
    "    res -= 14 ** d * 2 + 14 ** (d-1) * 13\n",
    "    res += 13 ** d\n",
    "\n",
    "print(hex(16 ** N - 1 - res)[2:].upper())\n",
    "print(res)"
   ]
